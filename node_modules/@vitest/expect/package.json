  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@standard-schema/spec": "^1.0.0",
    "@types/chai": "^5.2.2",
    "chai": "^6.0.1",
    "tinyrainbow": "^3.0.3",
    "@vitest/spy": "4.0.5",
    "@vitest/utils": "4.0.5"
  },
  "devDependencies": {
    "@vitest/runner": "4.0.5"
  },
  "scripts": {
    "build": "premove dist && rollup -c",
    "dev": "rollup -c --watch"
  }
}